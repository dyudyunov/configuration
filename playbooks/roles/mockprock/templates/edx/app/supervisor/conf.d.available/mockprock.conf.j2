# {{ ansible_managed }}

[program:{{ mockprock_service_name }}]

command={{ mockprock_app_dir }}/mockprock.sh
user={{ common_web_user }}
directory={{ mockprock_code_dir }}
stdout_logfile={{ supervisor_log_dir }}/%(program_name)s-stdout.log
stderr_logfile={{ supervisor_log_dir }}/%(program_name)s-stderr.log
killasgroup=true
stopasgroup=true
